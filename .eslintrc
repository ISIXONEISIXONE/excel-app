 {
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
    "linebreak-style": ["error", "windows"],
    "semi": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off"
    },
"extends": ["eslint:recommended", "google"]
}